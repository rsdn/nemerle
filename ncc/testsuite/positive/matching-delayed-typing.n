using System;
using System.Console;

module Program
{
  Main () : void
  {
    mutable i = 0;
    def someId = Guid();
    def someId2 = Guid("{9CFC8425-8E4D-4C1E-8CDD-A9219664FD89}");

    def res1 =
      (match (i)
      {
        | 0 => someId;
        | 1 => someId2;
        | _ => null;
      })  : _?;

    WriteLine(res1.Value);
    i++;

    def res2 : _? =
      match (i)
      {
        | 0 => someId;
        | 1 => someId2;
        | _ => null;
      };

    WriteLine(res2.Value);
    i++;

    def res3 =
      match (i)
      {
        | 0 => someId : _?;
        | 1 => someId2;
        | _ => null;
      };

    WriteLine(res3.HasValue);
  }
}

