#pragma indent

using System.Console

module M

  Main() : void

    def x1(p : int)
      p + 2

    def x2(p : int)
      p * 2

    def x3(p : int)
      p + 3

    WriteLine($"$(x1 >> x2 <| 2)")
    WriteLine($"$(x1 << x2 <| 2)")

    WriteLine($"$(3 |> x1 >> x2)")
    WriteLine($"$(3 |> x1 << x2)")

    WriteLine($"$(4 |> (x1 >> x2) >> x3)")
    WriteLine($"$(4 |> x1 >> (x2 >> x3))")
    WriteLine($"$(4 |> (x1 << x2) << x3)")
    WriteLine($"$(4 |> x1 << (x2 << x3))")

/*
BEGIN-OUTPUT
8
6
10
8
