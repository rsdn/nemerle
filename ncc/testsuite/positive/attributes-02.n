using System;
using System.Console;
using System.Reflection;

[Record]
public class Attr : Attribute
{
  public Value : int;
}

[Attr(ConstText + 6)]
module Program
{
  public ConstText : int = 1 + 2;
  Main() : void
  {
    def attr = Attr.GetCustomAttribute(typeof(this), typeof(Attr)) :> Attr;
    WriteLine(attr.Value);
  }
}

