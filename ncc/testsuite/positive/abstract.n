abstract class C1 {
  abstract public m1 () : void;
  public m2 () : void
  {
    m1 ();
  }
}

class C2 : C1 {
  public override m1 () : void
  {
    System.Console.WriteLine ("C2.m1");
  }

  public this () {}
}

interface I {
  mi () : void;
}

class CI1 : I {
  public virtual mi () : void
  { 
    System.Console.WriteLine ("CI1.mi");
  }

  public this () {}
}

class CI2 : CI1 {
  public override mi () : void
  { 
    System.Console.WriteLine ("CI2.mi");
  }
  
  public this () {}
}

module M {
  Main () : void 
  {
    def c = C2 ();
    c.m1 ();
    (c : C1).m1 ();
    c.m2 ();
    (c : C1).m2 ();

    (CI1 () : I).mi ();
    (CI2 () : I).mi ();
  }
}

/*
BEGIN-OUTPUT
C2.m1
C2.m1
C2.m1
C2.m1
CI1.mi
CI2.mi
END-OUTPUT
*/
