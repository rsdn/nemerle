// REFERENCE: REFERENCE: System.Core

using System.Console;
using System.Linq;

module Program
{
  public static Id[T](x : T) : T { x }

  Main() : void
  {
    def res = [1,2,3].Select(x => x).ToDictionary(Id, x => x.ToString()); //ICE
    WriteLine($<#..$(res.Keys; ", "; k => $"'$k'=$(res[k])")#>);
  }
}

/*
BEGIN-OUTPUT
'1'=1, '2'=2, '3'=3
END-OUTPUT
*/
