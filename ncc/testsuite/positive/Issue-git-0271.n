f ==>
f.HasValue ==> False
unchecked (f :> ?int).HasValue ==> False
f ==> 42,12345
f.HasValue ==> True
unchecked (f :> ?int) ==> 42
unchecked (f :> ?int).HasValue ==> True
END-OUTPUT
