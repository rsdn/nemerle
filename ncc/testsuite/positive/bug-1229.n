using System.Console;

class Foo { }

variant Wrapper
{
  | FooClass { foo : Foo; }
  | AnyStruct { obj : object }

  public static @:(foo : Foo)  : Wrapper                  { FooClass(foo) }
  public static @:[T](obj : T) : Wrapper where T : struct { AnyStruct(obj) }
}

module Program
{
  Main() : void
  {
    def w : Wrapper = Foo();
    WriteLine(w.GetType().Name);

    def w : Wrapper = 10;
    WriteLine(w.GetType().Name);
  }
}

/*
BEGIN-OUTPUT
FooClass
AnyStruct
END-OUTPUT
*/  