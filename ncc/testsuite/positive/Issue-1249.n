using System;
using System.Console;
using SCG = System.Collections.Generic;

using Nemerle.Imperative;

module Program {

  Main() : void {
    def xs = array.[2][["Nemerle", "C#", "C++"], ["VB", "Delphi", "Lisp"]];

    foreach (x in xs with i)
    {
      when(x.StartsWith("C"))
        continue;
      when(x == "Delphi")
        break;
      WriteLine($"$(i + 1). $x");
    }
  }
}
/*
BEGIN-OUTPUT
1. Nemerle
4. VB
END-OUTPUT
*/