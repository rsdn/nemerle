using System;
using System.Console;
using Nemerle.Utility;

module Program
{
  Main() : void
  {
    WriteLine("str".Fold(("", 0), fun(_chr, _acc) { ("test1", 1) })[0]);
        
    def FormatCStyleText(text)
    {
      text.Fold(("", 0), (_chr, _acc) => ("test2", 2))[1]
    }
    
    WriteLine(FormatCStyleText("aa"));
    
    def tuple = (3, "3");
    
    WriteLine(tuple[0]);
  }
}

