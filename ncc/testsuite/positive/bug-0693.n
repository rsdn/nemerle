using System;
using System.Console;

module Program {

  num1 = 1;
  num2 : int = num1 %| 2;

  Main() : void {
    def checkConstant(t) {
      foreach (field when !field.IsLiteral in t.GetFields())
        WriteLine($"$(field.Name) is not a constant");
    }

    // Read only
    checkConstant(typeof(Program));

    WriteLine(num1);
    WriteLine(num2);
  }

}
/*
BEGIN-OUTPUT
1
3
END-OUTPUT
*/