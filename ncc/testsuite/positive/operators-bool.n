using System;
using System.Console;

module Program
{
  Main() : void
  {
    Check(true);
    WriteLine("");
    Check(false);
  }
  
  Check(v : bool) : void
  {
    WriteLine($"v = $v");
    
    LogicalAnd(v);
    LogicalOr(v);    
    BitwiseAnd(v);
    BitwiseOr(v);
  }
  
  LogicalAnd(v : bool) : void
  {
    mutable val;
    
    WriteLine(" v &&= x");
    val = v;
    val &&= TruePrint();
    val = v;
    val &&= FalsePrint();
    WriteLine("");
        
    WriteLine(" v = v && x");
    val = v;
    val = val && TruePrint();    
    val = v;
    val = val && FalsePrint();
    WriteLine("");
  }
  
  LogicalOr(v : bool) : void
  {
    mutable val;
      
    WriteLine(" v ||= x");
    val = v;
    val ||= TruePrint();
    val = v;
    val ||= FalsePrint();
    WriteLine("");
    
    WriteLine(" v = v || x");
    val = v;
    val = val || TruePrint();
    val = v;
    val = val || FalsePrint();
    WriteLine("");
  }
  
  BitwiseAnd(v : bool) : void
  {
    mutable val;
    
    WriteLine(" v &= x");
    val = v;
    val &= TruePrint();
    val = v;
    val &= FalsePrint();
    WriteLine("");
    
    WriteLine(" v = v & x");
    val = v;
    val = val & TruePrint();
    val = v;
    val = val & FalsePrint();
    WriteLine("");
  }
  
  BitwiseOr(v : bool) : void
  {
    mutable val;
    
    WriteLine(" v |= x");
    val = v;
    val |= TruePrint();
    val = v;
    val |= FalsePrint();
    WriteLine("");
    
    WriteLine(" v = v | x");
    val = v;
    val = val | TruePrint();
    val = v;
    val = val | FalsePrint();
    WriteLine("");
  }
  
  TruePrint() : bool
  {
    WriteLine("  TruePrint");
    true
  }
  
  FalsePrint() : bool
  {
    WriteLine("  FalsePrint");
    false
  }

}

