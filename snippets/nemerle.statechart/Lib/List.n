    not_empty_err = "list must be not empty";

    public SplitLast[T](this lst : list[T]) : T * list[T]
    {
      def loop(lst, res)
      {
        match (lst)
        {
          | []           => throw ArgumentException(not_empty_err)
          | [head]       => (head, res.Rev())
          | head :: tail => loop(tail, head :: res)
        }
      }

      loop(lst, [])
    }

    public Last[T](this lst : list[T]) : T
    {
      def loop(lst)
      {
        | [] => throw ArgumentException(not_empty_err)
        | [head] => head
        | _ :: tail => loop(tail)
      }

      loop(lst)
    }

    public RemoveDupFirst[T](this lst : list[T], lst2 : list[T], eq_pred : T * T -> bool) : list[T] * list[T]
    {
      def loop(lst, lst2)
      {
        | (head :: tail, head2 :: tail2) when (eq_pred(head, head2)) => loop(tail, tail2)
        | (_, _)                                                     => (lst, lst2)
      }

      loop(lst, lst2)
    }

    public DupLast[T](this lst : list[T], lst2 : list[T], eq_pred : T * T -> bool) : T where T : class
    {
      def loop(lst, lst2, last)
      {
        match (lst, lst2)
        {
          | (head :: tail, head2 :: tail2) when (eq_pred(head, head2)) => loop(tail, tail2, head)
          | (_, _)                                                     => last
        }
      }

      loop(lst, lst2, null)
    }

    public DupLast[T](this lst : list[T], lst2 : list[T]) : T where T : class
    {
      def loop(lst, lst2, last)
      {
        match (lst, lst2)
        {
          | (head :: tail, head2 :: tail2) when (head : object == head2) => loop(tail, tail2, head)
          | (_, _)                                                       => last
        }
      }

      loop(lst, lst2, null)
    }
    
