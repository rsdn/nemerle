  [Record]
  public class Comparer[T] : IComparer[T]
  {
    public func : T * T -> int;
    
    public Equals(x : T, y : T) : bool { func(x, y) == 0 }
    
    public Compare(val1 : T, val2 : T) : int
    {
      func(val1, val2)
    }
    
  }
