    public IsSingle[T](this lst : IEnumerable[T]) : bool
    {
      def en = lst.GetEnumerator();
      en.MoveNext() && !en.MoveNext()
    }

    public IsMoreThanOne[T](this lst : IEnumerable[T]) : bool
    {
      def en = lst.GetEnumerator();
      en.MoveNext() && en.MoveNext()
    }

