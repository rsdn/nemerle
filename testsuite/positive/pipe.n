using System.Console

module M

  Main() : void

    def x(p : int)
      WriteLine(p)

    def y(p : int)
      p + 1

    1 |> x
    x <| 2

    3 |> fun(p)
      WriteLine(p)

    (fun(p) { WriteLine(p) }) <| 4

    4 |> y |> x
    x <| (y <| 5)

