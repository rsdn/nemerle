public abstract class Foo[T]
{
  public abstract Run(_ : T) : void;
}

public class Bar : Foo[int * int]
{
  public override Run(tuple : int * int) : void
  {
    System.Console.WriteLine(tuple);
  }
}

module Program
{
  Main() : void
  {
    def o : Foo[_] = Bar();
    
    o.Run(2, 3);
    o.Run((4, 5));
  }
}

/*
BEGIN-OUTPUT
(2, 3)
(4, 5)
END-OUTPUT
*/
