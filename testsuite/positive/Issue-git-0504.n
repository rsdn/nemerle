using System.Console;
using Nemerle.Imperative;
using Nemerle.Imperative.GoTo;

module Program
{
  Main(args : array[string]) : void
  {
    goto Start;
    label Label2;
    WriteLine("Label2:");
    goto End;
    label Start;
    WriteLine("Start:");
    
    def loop() : void
    {
    }
      
    loop();

    when (args.Length == 0)
      goto Label1;
    
    WriteLine("args.Length != 0");
    return;
    
    label Label1;
    WriteLine("Label1:");

    goto Label2;
    label End;
    WriteLine("End:");
  }
}

