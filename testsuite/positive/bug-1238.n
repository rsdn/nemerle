using System.Console;

module Program
{
  Main() : void
  {
    def test1()
    {
      WriteLine("test1()");
      
      def a = 0L : long?;
      mutable b; // have null value
      
      WriteLine($"b.HasValue=$(b.HasValue)");

      def res = a == b;
      
      b = 1 : int?;
      
      WriteLine($"res=$res");
    }
    
    def test2()
    {
      WriteLine("test2()");
      
      def a = 0L : long?;
      mutable b = null;
      
      WriteLine($"b.HasValue=$(b.HasValue)");

      def res = a == b;
      
      b = 1 : int?; // need for type inference
      
      WriteLine($"res=$res");
    }
    
    def test3()
    {
      WriteLine("test3()");
      
      def a = 0L : long?;
      mutable b = 1 : int?;
      
      WriteLine($"b.HasValue=$(b.HasValue)");

      def res = a == b;
      
      WriteLine($"res=$res");
    }
    
    def test4()
    {
      WriteLine("test4()");
      
      def a = 1L : long?;
      def b = 1 : int?;
      
      WriteLine($"b.HasValue=$(b.HasValue)");

      def res = a == b;
      
      WriteLine($"res=$res");
    }
    
    def test5()
    {
      WriteLine("test5()");
      
      def a = 1 : int?;
      def b = 1 : int?;
      
      WriteLine($"b.HasValue=$(b.HasValue)");

      def res = a == b;
      
      WriteLine($"res=$res");
    }

    test1();
    test2();
    test3();
    test4();
    test5();
  }
}

